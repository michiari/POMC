var  leftGEQright, loLThi, loEQhi, loEQleft, hiEQright, eqEQlo, eqEQleft, middle1;

main() {
	leftGEQright = false;
	qs3();
}

qs3() {
	if (leftGEQright) {
	} else {
		middle1 = false;
		loEQhi = false;
		loLThi = true;
		loEQleft = true;
		hiEQright = true;
		eqEQlo = true;
		eqEQleft = true;
		firstIteration3();
		while (loLThi || loEQhi) { 
			qsif3();
		}

		//recursive calls
		if (loEQleft){
			leftGEQright = true;
			qs3();
			leftGEQright = false;
			qs3();
		} else { 
			if (hiEQright && eqEQlo) {
				leftGEQright = false;
				qs3();
				leftGEQright = true;
				qs3();
			} else {
				if (hiEQright && eqEQleft) {
					leftGEQright = true;
					qs3();
					leftGEQright = true;
					qs3();
				} else {
					if (hiEQright) {
						randomleftLEQright();
						qs2();
						leftGEQright = true;
						qs3();
					} else {
						if (eqEQleft) {
							leftGEQright = true;
							qs3();
							randomleftLEQright();
							qs2();
						} else {
							if (eqEQlo) {
								// do not abstract too much
								leftGEQright = true;
								qs3();
								leftGEQright = false;
								qs2();
							} else {}
						} 
					}
				}
			}
		}
	}
}

firstIteration3() {
	eqEQlo = false;
	shiftloLEQhi3();
	loEQleft = false;
}

qsif3() {
	if (*) {
		shiftloLEQhi3();
		hiEQright = false;
	} else {
		if (*) {
			eqEQleft = false;
		} else {
			eqEQlo = false;
		}
		shiftloLEQhi3();
		loEQleft = false;	
	}
}



shiftloLEQhi3() {
	if (middle1) {
		if (loEQhi) {
			loEQhi = false;
			loLThi = false;					
		} else {
			loEQhi = true;
			loLThi = false;
		}
	} else { middle1 = true;}
}

randomleftLEQright(){
	if (*){
		leftGEQright = false;
	} else {
		leftGEQright = true;
	}
}

qs2() {
	if (leftGEQright) {
	} else {
		loEQhi = false;
		loLThi = true;
		loEQleft = true;
		hiEQright = true;
		eqEQlo = true;
		eqEQleft = true;
		firstIteration2();
		while (loLThi || loEQhi) { 
			qsif2();
		}

		//recursive calls
		if (loEQleft){
			leftGEQright = true;
			qs2();
			leftGEQright = false;
			qs2();
		} else { 
			if (hiEQright && eqEQlo) {
				leftGEQright = false;
				qs2();
				leftGEQright = true;
				qs2();
			} else {
				if (hiEQright && eqEQleft) {
					leftGEQright = true;
					qs2();
					leftGEQright = true;
					qs2();
				} else {
					if (hiEQright) {
						leftGEQright = true;
						qs2();
						leftGEQright = true;
						qs2();
					} else {
						if (eqEQleft) {
							leftGEQright = true;
							qs2();
							leftGEQright = true;
							qs2();
						} else {
							if (eqEQlo) {
								leftGEQright = true;
								qs2();
								leftGEQright = true;
								qs2();
							} else {}
						} 
					}
				}				
			}
		}
	}
}

qsif2() {
	if (*) {
		shiftloLEQhi2();
		hiEQright = false;
	} else {
		if (*) {
			eqEQleft = false;
		} else {
			eqEQlo = false;
		}
		shiftloLEQhi2();
		loEQleft = false;	
	}
}

firstIteration2() {
	eqEQlo = false;
	shiftloLEQhi2();
	loEQleft = false;
}

shiftloLEQhi2() {
	if (loEQhi) {
		loEQhi = false;
		loLThi = false;					
	} else {
		loEQhi = true;
		loLThi = false;
	}
}